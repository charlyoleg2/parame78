{
	"name": "desi78-ui",
	"description": "sveltekit-ui for desi78",
	"version": "0.1.7",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame78.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame78/",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"scripts": {
		"cp_pgdsvg": "fse copy ../../node_modules/desi78/dist/pgdsvg static/pgdsvg",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "rimraf .svelte-kit build node_modules static/pgdsvg"
	},
	"dependencies": {
		"desi78": "^0.5.29",
		"geometrix": "^0.5.49",
		"geomui": "^0.5.51"
	},
	"devDependencies": {
		"@atao60/fse-cli": "^0.1.9",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.7.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"globals": "^15.11.0",
		"npm-run-all2": "^6.2.4",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"rimraf": "^6.0.1",
		"sass": "^1.80.3",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.10.0",
		"vite": "^5.4.9",
		"vitest": "^2.1.3"
	}
}
